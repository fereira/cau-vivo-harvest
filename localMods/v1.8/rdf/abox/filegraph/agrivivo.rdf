    <rdfs:isDefinedBy rdf:resource="http://www.agrivivo.net/ontology/agrivivo"/>
    <rdfs:label>Agricultural Extension</rdfs:label>
    <rdfs:comment>Organizations of this type work in extension services, advisory services, information services.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
  </rdfs:Class>

